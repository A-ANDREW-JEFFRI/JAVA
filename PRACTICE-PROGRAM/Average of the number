package pro;
import java.util.*;
public class program 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int c=0,a=0;
		System.out.println("Enter the count of the element :");
		int n=sc.nextInt();
		int arr[]= new int[n];
		System.out.println("Enter the element :");
		for(int i=0;i<n;i++)
			arr[i]=sc.nextInt();
		for(int j=0;j<n;j++)
		{
			c+=arr[j];
			a++;
		}
		int b=c/a;
		System.out.println("Average of the "+n+" element is "+b);
	}
}
