package practice;
import java.util.*;
public class four{
    public static void main(String arg[])
    {
      Scanner a= new Scanner(System.in);
      int b= a.nextInt();
      int c= a.nextInt();
      System.out.println(b+"+"+c+"="+(b+c));
      System.out.println(b+"-"+c+"="+(b-c));
      System.out.println(b+"*"+c+"="+(b*c));
      System.out.println(b+"/"+c+"="+(b/c));
      System.out.println(b+"mod"+c+"="+(b%c));
    }
}
